{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules"
  ],
  "verbose": true,
  "execMap": {
    "": "node",
    "ts": "ts-node",
    "js": "node --harmony"
  },
  "events": {
  },
  "watch": [
    "src",
    "packages/*/lib/**/*.js"
  ],
  "env": {
    "NODE_ENV": "development",
    "PORT": "4000"
  },
  "ext": "js json ts",
  "legacy-watch": false
}
